<div class="form-group">
  <label class="col-lg-2 control-label">Number*</label>

  <div class="col-lg-10">
    <input type="text" name="number" class="form-control" placeholder="Number" autocomplete="off"
           value="<%= consultant.number %>">
  </div>
</div>


<div class="form-group">
  <label class="col-lg-2 control-label">Name*</label>

  <div class="col-lg-10">
    <input type="text" name="name" class="form-control" placeholder="Consultant name" autocomplete="off"
           value="<%= consultant.name %>">
  </div>
</div>


<div class="form-group">
  <label class="col-lg-2 control-label">Description*</label>

  <div class="col-lg-10">
    <textarea class="form-control" name="description" rows="6"
              placeholder="Description"><%= consultant.description %></textarea>
  </div>
</div>
